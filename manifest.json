{
  "manifest_version": 2,

  "name": "Better Web",
  "description": "This extension suppresses junky content mill sites.",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["http://*.variety.com/*"],
      "css": ["better.css"],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "activeTab"
  ]
}
